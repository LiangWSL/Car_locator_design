.check_container{
  display: flex;
  flex-direction: column;
  padding: 20rpx;
  background-color: #f3f3f3;
}
.order_d{
  display: flex;
  flex-direction: column;
  margin: 10rpx;
}
.order-id {
  display: flex;
  flex-direction: row;
  border-bottom: solid 1px #e1e1e1;
  margin: 10rpx;
  margin-bottom: 0rpx;
  align-items: center;
  justify-content: space-between;
  padding: 10rpx;
  background: #ffffff;
  font-size: 20rpx;
  border-radius: 10rpx;
}
.order_detail {
  display: flex;
  flex-direction: column;
  border-bottom: solid 1px #e1e1e1;
  margin: 10rpx;
  margin-top: 0rpx;
  padding: 10rpx;
  background: #ffffff;
  font-size: 36rpx;
  border-radius: 10rpx;
}
